all: clean pngtest

main.o: main.cpp
	"/usr/bin/g++" $(CXXFLAGS) -c $(shell "/usr/local/src/lnmpp/cmake-2.8.7/bin/cmake" --find-package -DCMAKE_MODULE_PATH="/usr/local/src/lnmpp/cmake-2.8.7/Tests/FindPackageModeMakefileTest" -DNAME=Foo -DLANGUAGE=CXX -DCOMPILER_ID=GNU -DMODE=COMPILE) main.cpp

pngtest: main.o
	"/usr/bin/g++" $(LDFLAGS) -o pngtest main.o $(shell "/usr/local/src/lnmpp/cmake-2.8.7/bin/cmake" --find-package -DCMAKE_MODULE_PATH="/usr/local/src/lnmpp/cmake-2.8.7/Tests/FindPackageModeMakefileTest" -DNAME=Foo -DLANGUAGE=CXX -DCOMPILER_ID=GNU -DMODE=LINK)

clean:
	rm -f *.o pngtest
